{
  "name": "node-fork-queue",
  "version": "0.1.4",
  "description": "A queue for node.js that uses child processes (child_process.fork()). This queue uses Pools for child-process because it takes time to spawn new child-process.",
  "author": "Ha Nguyen <havietnguyen.it@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/hanguyen-it/node-fork-queue.git"
  },
  "main": "lib/index.js",
  "scripts": {
    "test": "nodemon test/MainProcess.ts --exec babel-node",
    "type-check": "npx tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js",
    "build:types": "npx tsc",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline"
  },
  "dependencies": {
    "@types/generic-pool": "^3.1.9",
    "redis": "^3.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@babel/types": "^7.12.12",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-watch": "^7.0.0",
    "jest": "^26.6.3",
    "mock-fs": "^4.13.0",
    "nodemon": "^2.0.6",
    "prettier": "^2.2.1",
    "redis-mock": "^0.55.0",
    "typescript": "^4.1.3"
  },
  "keywords": [
    "child_process",
    "child_process.fork()",
    "fork queue",
    "fork pool",
    "fork",
    "queue",
    "typescript",
    "nodejs"
  ],
  "license": "MIT"
}
